{
  "name": "pretend",
  "version": "0.1.0",
  "dependencies": {
    "angular": "1.2.13",
    "angular-mocks": "1.2.13"
  },
  "homepage": "https://github.com/basslagter/pretend",
  "authors": [
    "Bas Slagter"
  ],
  "description": "Small and simple mocking 'framework' to make it easier to mock items when writing unit tests for an Angular app.",
  "main": "./dist/pretend-min.js",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "unit",
    "testing",
    "jasmine",
    "angular",
    "mocking",
    "mock"
  ],
  "license": "MIT",
  "ignore": [
    ".gitignore",
    "node_modules",
    "bower_components",
    "tests",
	"scripts",
	"bower.json",
	"Gruntfile.js",
	"package.json",
    "karma-unit.conf.js",
    "karma-integration.conf.js"
  ]
}
